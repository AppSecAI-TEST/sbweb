<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wtyt.demo.mapper.DemoMapper">

    <insert id="insDemoBean" parameterType="DemoSendBean">
        INSERT
        INTO
        T_SB_PRO
        (ID,
        PRO_NAME,
        PRO_CODE)
        VALUES
        (SE_SB_PRO_ID.NEXTVAL,
        #{pro_name},
        #{pro_code})
    </insert>

</mapper>

